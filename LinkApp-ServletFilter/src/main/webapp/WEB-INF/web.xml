<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<display-name>LinkApp-ServletFilter</display-name>
	<welcome-file-list>
		<welcome-file>links.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>link</servlet-name>
		<servlet-class>com.harsh.servlet.LinksServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>link</servlet-name>
		<url-pattern>/linksurl</url-pattern>
	</servlet-mapping>

</web-app>