<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>DBPoolApp</display-name>
  <welcome-file-list>
    <welcome-file>customer_register.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
		<servlet-name>cust</servlet-name>
		<servlet-class>com.harsh.servlet.CustomerRegisterServlet</servlet-class>
		
		<init-param>
			<param-name>DBjndi-inti</param-name>
			<param-value>java:/comp/env/DSjndi</param-value>
		</init-param>
	</servlet>

	<!-- servlet mapping configuration -->
	<servlet-mapping>
		<servlet-name>cust</servlet-name>
		<url-pattern>/custurl</url-pattern>
	</servlet-mapping>
</web-app>